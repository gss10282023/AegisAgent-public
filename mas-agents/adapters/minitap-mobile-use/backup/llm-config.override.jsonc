// Your custom LLM configuration
{
  "planner": {
    "provider": "openrouter",
    "model": "openai/gpt-5-mini",
    "fallback": {
      "provider": "openrouter",
      "model": "openai/gpt-5-mini"
    }
  },
  "orchestrator": {
    "provider": "openrouter",
    "model": "openai/gpt-5-mini",
    "fallback": {
      "provider": "openrouter",
      "model": "openai/gpt-5-mini"
    }
  },
  "contextor": {
    // Optional: Only needed if using app lock feature
    "provider": "openrouter",
    "model": "openai/gpt-5-mini",
    "fallback": {
      "provider": "openrouter",
      "model": "openai/gpt-5-mini"
    }
  },
  "cortex": {
    // IMPORTANT: Needs vision capabilities!
    // Recommended: gpt-4o, gpt-5, or equivalent vision-capable model
    "provider": "openrouter",
    "model": "openai/gpt-5",
    "fallback": {
      "provider": "openrouter",
      "model": "openai/gpt-5"
    }
  },
  "executor": {
    "provider": "openrouter",
    "model": "openai/gpt-5-mini",
    "fallback": {
      "provider": "openrouter",
      "model": "openai/gpt-5-mini"
    }
  },
  "utils": {
    "hopper": {
      // Needs at least a 256k context window
      "provider": "openrouter",
      "model": "openai/gpt-5-mini",
      "fallback": {
        "provider": "openrouter",
        "model": "openai/gpt-5-mini"
      }
    },
    "outputter": {
      "provider": "openrouter",
      "model": "openai/gpt-5-mini",
      "fallback": {
        "provider": "openrouter",
        "model": "openai/gpt-5-mini"
      }
    }
  }
}
